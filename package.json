{
    "name": "reactapp",
    "version": "0.0.0",
    "description": "React/Node sandbox",
    "main": "app.js",
    "engines": {
        "node": "8.9.4",
        "npm": "5.6.0"
    },
    "scripts": {
        "start": "node bin/www",
        "server": "set DEBUG=reactapp:* & nodemon bin/www",
        "client": "set PORT=3060 & npm run start --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/CountryGuide/React-Sandbox.git"
    },
    "author": "Ivan Yakovlev",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/CountryGuide/React-Sandbox/issues"
    },
    "homepage": "https://github.com/CountryGuide/React-Sandbox#readme",
    "dependencies": {
        "body-parser": "^1.18.2",
        "concurrently": "^3.5.1",
        "cookie-parser": "^1.4.3",
        "debug": "^3.1.0",
        "express": "^4.16.3",
        "morgan": "^1.9.0",
        "pug": "^2.0.3",
        "serve-favicon": "^2.5.0"
    },
    "devDependencies": {
        "create-react-app": "^1.5.2",
        "nodemon": "^1.17.3"
    }
}
